{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": [false],
    "prefer-const": true,
    "semicolon": [true, "always"],
    "whitespace": [true, "check-module", "check-branch", "check-decl", "check-operator", "check-preblock", "check-separator"],
    "quotemark": [true, "single", "jsx-double", "avoid-escape"],
    "no-empty": [true, "allow-empty-catch", "allow-empty-functions"],
    "no-shadowed-variable": [true, {"temporalDeadZone" : false}],
    "max-line-length": [true, {"limit": 120, "ignore-pattern": "^import |^export {(.*?)}"}],
    "ter-arrow-parens": [true, "as-needed"],
    "align": [true, "parameters", "statements", "members", "elements"],
    "interface-name": false,
    "ordered-imports": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "no-console": false,
    "no-string-literal": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "variable-name": false,
    "import-name": false,
    "no-boolean-literal-compare": false,
    "member-ordering": false,
    "array-type": [true, "array"],
    "only-arrow-functions": false
  },
  "rulesDirectory": []
}